# Detect if a runner falls
execute as @a[gamemode=adventure] run scoreboard players operation @s TMP = @s journey_level
execute as @a[gamemode=adventure] run scoreboard players remove @s TMP 1
execute as @a[gamemode=adventure] at @s if entity @s[y=200,dy=100] run scoreboard players set @s journey_fall 0
execute as @a[gamemode=adventure] at @s if entity @s[y=0,dy=200] run scoreboard players set @s journey_fall 1
execute as @a[gamemode=adventure] at @s if entity @s[y=0,dy=200] run tellraw @s {"text":"正在传送回上一记录点...","color":"red"}
execute as @a[gamemode=adventure] at @s if entity @s[y=0,dy=200] run playsound block.note_block.pling master @s ~ ~ ~ 1 0.5
execute as @e[tag=checkpoint] if score @s journey_level = @a[scores={journey_fall=1},limit=1] TMP run tp @a[scores={journey_fall=1},limit=1] @s

# Particles
execute as @e[tag=checkpoint,scores={journey_level=..32}] at @s run particle minecraft:dust 1 1 0 1 ~ ~ ~ 0.25 1 0.25 0.1 3
execute as @e[tag=checkpoint,scores={journey_level=33}] at @s run particle minecraft:dust 0 1 0 1 ~ ~ ~ 0.25 1 0.25 0.1 3

# Checkpoints
execute as @a[gamemode=adventure] at @s if score @e[tag=checkpoint,distance=..1.5,limit=1,scores={journey_level=..32}] journey_level = @s journey_level run spawnpoint @s ~ ~ ~ ~
execute as @a[gamemode=adventure] at @s if score @e[tag=checkpoint,distance=..1.5,limit=1,scores={journey_level=..32}] journey_level = @s journey_level run scoreboard players add @s tick_elapsed 500
execute as @a[gamemode=adventure] at @s if score @e[tag=checkpoint,distance=..1.5,limit=1,scores={journey_level=..32}] journey_level = @s journey_level run title @s subtitle [{"text":"+25s Time!","color":"gold"}]
execute as @a[gamemode=adventure] at @s if score @e[tag=checkpoint,distance=..1.5,limit=1,scores={journey_level=..32}] journey_level = @s journey_level run tellraw @a [{"text":"[⭐] "},{"selector":"@s","color":"green"},{"text":" 到达了第 ","color":"white"},{"score":{"name":"@s","objective":"journey_level"},"color":"gold"},{"text":" 个记录点！"}]
gamemode spectator @a[scores={journey_level=34}]

# Destination
execute as @a at @s if score @e[tag=checkpoint,distance=..1.5,limit=1,scores={journey_level=33}] journey_level = @s[gamemode=adventure] journey_level run title @s title [{"text":"祝贺!","color":"gold","bold":true}]
execute as @a at @s if score @e[tag=checkpoint,distance=..1.5,limit=1,scores={journey_level=33}] journey_level = @s[gamemode=adventure] journey_level run title @s subtitle [{"text":"你完成了全部跑酷挑战!","color":"yellow"}]
execute as @a at @s if score @e[tag=checkpoint,distance=..1.5,limit=1,scores={journey_level=33}] journey_level = @s[gamemode=adventure] journey_level run tellraw @a [{"text":"[⭐] "},{"selector":"@s","color":"green"},{"text":" 到达了旅途终点！"}]
execute as @a at @s if score @e[tag=checkpoint,distance=..1.5,limit=1,scores={journey_level=33}] journey_level = @s[gamemode=adventure] journey_level run gamemode spectator @s

